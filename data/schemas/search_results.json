{
    "name":"search_results",
    "description":"Contains the paginated results of a search query",
    "type":"object",
    "properties": {
        "total_results" : {
            "type":"integer",
            "required":true,
            "minimum":0
        },
        "total_pages" : {
            "type":"integer",
            "required":true,
            "minimum":0
        },
        "current_page" : {
            "type":"integer",
            "required":true,
            "minimum":0
        },
        "items_per_page" : {
            "type":"integer",
            "required":true,
            "minimum":0
        },
        "results": {
            "type":"array",
            "items": {
                "type": "search_result"
            }
        }
    }
}
    
